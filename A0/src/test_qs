 .data

# Test data.  If you change the number of words, make sure to also
# change the argument passed in a2 to partition.

array:
        .word 100
        .word 100
        .word 100000
        .word 255
        .word 0
        .word 123
        .word 4
        .word 50
        .word 1000
        .word 1
        .word 1451345

.text
test:
        la a0, array      # address of array
        addi a1, zero, 0  # start element
        addi a2, zero, 11  # number of elements
        jal ra, quicksort # call quicksort
        jal zero, end     # jump to end

.include "quicksort.s"

.text

end: